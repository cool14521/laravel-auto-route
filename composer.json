{
    "name": "web-expendables/laravel-auto-route",
    "description": "Declare automatically route names for your project following conventions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Yung",
            "email": "stevenyung@icloud.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.5",
        "illuminate/routing": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Expendables\\AutoRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Expendables\\AutoRoute\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Expendables\\AutoRoute\\AutoRouteServiceProvider"
            ]
        }
    }
}
